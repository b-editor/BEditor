<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:BEditor.WPF.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/CheckPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ColorPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/DocumentPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/SelectorPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/FontPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ValuePropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/TextPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/FolderPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/LabelComponentView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ButtonComponentView.xaml" />

        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ExpandTree.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ColorPicker.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="{x:Type local:CheckPropertyView}" BasedOn="{StaticResource CheckPropertyStyle}" />
    <Style TargetType="{x:Type local:ColorPropertyView}" BasedOn="{StaticResource ColorPropertyStyle}" />
    <Style TargetType="{x:Type local:DocumentPropertyView}" BasedOn="{StaticResource DocumentPropertyStyle}" />
    <Style TargetType="{x:Type local:SelectorPropertyView}" BasedOn="{StaticResource SelectorPropertyStyle}" />
    <Style TargetType="{x:Type local:FontPropertyView}" BasedOn="{StaticResource FontPropertyStyle}" />
    <Style TargetType="{x:Type local:ValuePropertyView}" BasedOn="{StaticResource ValuePropertyStyle}" />
    <Style TargetType="{x:Type local:TextPropertyView}" BasedOn="{StaticResource TextPropertyStyle}" />
    <Style TargetType="{x:Type local:FolderPropertyView}" BasedOn="{StaticResource FolderPropertyStyle}" />
    <Style TargetType="{x:Type local:LabelComponentView}" BasedOn="{StaticResource LabelComponentStyle}" />
    <Style TargetType="{x:Type local:ButtonComponentView}" BasedOn="{StaticResource ButtonComponentStyle}" />

    <Style TargetType="{x:Type local:ExpandTree}" BasedOn="{StaticResource ExpandTreeStyle}" />
    <Style TargetType="{x:Type local:ColorPicker}" BasedOn="{StaticResource ColorPickerStyle}" />
</ResourceDictionary>
