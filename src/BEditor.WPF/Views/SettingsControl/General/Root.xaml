<UserControl
    x:Class="BEditor.Views.SettingsControl.General.Root"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:data="clr-namespace:BEditor.Data;assembly=BEditor.Core"
    xmlns:local="clr-namespace:BEditor.Views.SettingsControl.General"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:resources="clr-namespace:BEditor.Properties"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <VirtualizingStackPanel>

        <md:ColorZone Content="{x:Static resources:Strings.General}" Style="{StaticResource SettingsColorZone}" />

    </VirtualizingStackPanel>
</UserControl>
