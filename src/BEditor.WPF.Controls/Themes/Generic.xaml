<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:BEditor.WPF.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/CheckPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ColorPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/DocumentPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/SelectorPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/SelectorPropertyViewGen.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ValuePropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/TextPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/FilePropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/FolderPropertyView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/LabelComponentView.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ButtonComponentView.xaml" />

        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ExpandTree.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/ColorPicker.xaml" />
        <ResourceDictionary Source="/BEditor.WPF.Controls;component/Themes/Seekbar.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource CheckPropertyStyle}" TargetType="{x:Type local:CheckPropertyView}" />
    <Style BasedOn="{StaticResource ColorPropertyStyle}" TargetType="{x:Type local:ColorPropertyView}" />
    <Style BasedOn="{StaticResource DocumentPropertyStyle}" TargetType="{x:Type local:DocumentPropertyView}" />
    <Style BasedOn="{StaticResource SelectorPropertyStyle}" TargetType="{x:Type local:SelectorPropertyView}" />
    <Style BasedOn="{StaticResource SelectorPropertyGenStyle}" TargetType="{x:Type local:SelectorPropertyViewGen}" />
    <Style BasedOn="{StaticResource ValuePropertyStyle}" TargetType="{x:Type local:ValuePropertyView}" />
    <Style BasedOn="{StaticResource TextPropertyStyle}" TargetType="{x:Type local:TextPropertyView}" />
    <Style BasedOn="{StaticResource FilePropertyStyle}" TargetType="{x:Type local:FilePropertyView}" />
    <Style BasedOn="{StaticResource FolderPropertyStyle}" TargetType="{x:Type local:FolderPropertyView}" />
    <Style BasedOn="{StaticResource LabelComponentStyle}" TargetType="{x:Type local:LabelComponentView}" />
    <Style BasedOn="{StaticResource ButtonComponentStyle}" TargetType="{x:Type local:ButtonComponentView}" />

    <Style BasedOn="{StaticResource ExpandTreeStyle}" TargetType="{x:Type local:ExpandTree}" />
    <Style BasedOn="{StaticResource ColorPickerStyle}" TargetType="{x:Type local:ColorPicker}" />
    <Style BasedOn="{StaticResource SeekbarStyle}" TargetType="{x:Type local:Seekbar}" />
</ResourceDictionary>
