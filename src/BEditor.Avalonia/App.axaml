<Application
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:convert="using:BEditor.Converters"
  x:Class="BEditor.App">
  <Application.Resources>
    <convert:CursorTypeToCursorConverter x:Key="CursorTypeToCursorConverter"/>
    <convert:SceneToTimelineConverter x:Key="SceneToTimelineConverter"/>
    <convert:PluginNameConverter x:Key="NameConvert"/>
    <convert:PluginAuthorConverter x:Key="AuthorConvert"/>
    <convert:ClipPropertyConverter x:Key="ClipPropertyConverter"/>
    <convert:EffectPropertyConverter x:Key="EffectPropertyConverter"/>
    <convert:EasingPropertyConverter x:Key="EasingPropertyConverter"/>
  </Application.Resources>
  <Application.Styles>
    <StyleInclude Source="avares://BEditor.Avalonia/Controls/SeekSlider.axaml" />
    <StyleInclude Source="avares://BEditor.Avalonia/Controls/Sidebar.axaml" />
    <StyleInclude Source="avares://BEditor.Avalonia/Controls/FluentUiIcons_Filled1.axaml" />
    <StyleInclude Source="avares://BEditor.Avalonia/Controls/FluentUiIcons_Filled2.axaml" />
    <StyleInclude Source="avares://BEditor.Avalonia/Controls/FluentUiIcons_Regular1.axaml" />
    <StyleInclude Source="avares://BEditor.Avalonia/Controls/FluentUiIcons_Regular2.axaml" />
    <StyleInclude Source="avares://BEditor.Avalonia/Views/Settings/SettingsResources.axaml" />
  </Application.Styles>
</Application>
